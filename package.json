{
  "name": "wicgate-nuxt",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "build:staging": "nuxt build --dotenv .env.staging",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "build:icons": "tsx scripts/generate-pwa-icons.ts",
    "build:og": "node --experimental-strip-types scripts/generate-og-images.ts",
    "build:sitemap": "tsx scripts/generate-sitemap.ts",
    "prepare": "husky",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "typecheck": "nuxt typecheck",
    "test:thorough": "cross-env TEST_REAL_TIMERS=true vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-brands-svg-icons": "^7.0.1",
    "@fortawesome/free-regular-svg-icons": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@pinia/nuxt": "^0.9.0",
    "@sentry/vue": "^10.17.0",
    "axios": "^1.13.2",
    "nuxt": "^3.15.0",
    "pinia": "^2.3.0",
    "vue": "^3.4.29",
    "vue-advanced-cropper": "^2.8.9",
    "vue-router": "^4.3.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.3.0",
    "@nuxtjs/tailwindcss": "^6.13.2",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@unhead/vue": "^1.11.18",
    "@vite-pwa/nuxt": "^0.10.6",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.19",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.0",
    "happy-dom": "^20.1.0",
    "husky": "^9.1.7",
    "postcss": "^8.4.38",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.92.1",
    "sharp": "^0.34.5",
    "svgo": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "tsx": "^4.20.6",
    "typescript": "^5.4.5",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.1"
  }
}
