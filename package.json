{
  "name": "wicgate-vue",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:icons && npm run build:sitemap && vite-ssg build && npm run postbuild:head && npm run validate:schemas",
    "build:icons": "tsx scripts/generate-pwa-icons.ts",
    "build:sitemap": "tsx scripts/generate-sitemap.ts",
    "postbuild:head": "tsx scripts/apply-head-meta.ts",
    "validate:build": "tsx scripts/validate-build.ts",
    "validate:schemas": "tsx scripts/validate-schemas.ts",
    "prepare": "husky",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.vue src",
    "lint:fix": "eslint --ext .ts,.vue src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:thorough": "cross-env TEST_REAL_TIMERS=true vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-brands-svg-icons": "^7.0.1",
    "@fortawesome/free-regular-svg-icons": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@sentry/vue": "^10.17.0",
    "pinia": "^3.0.3",
    "vue": "^3.4.29",
    "vue-router": "^4.3.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@unhead/vue": "^1.11.18",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.19",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.0",
    "happy-dom": "^19.0.2",
    "husky": "^9.1.7",
    "postcss": "^8.4.38",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.92.1",
    "sharp": "^0.34.4",
    "svgo": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "tsx": "^4.20.6",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "vite-plugin-image-optimizer": "^2.0.2",
    "vite-plugin-pwa": "^1.0.3",
    "vite-ssg": "^28.1.0",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0"
  }
}
